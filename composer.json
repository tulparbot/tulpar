{
    "name": "isaeken/tulpar-bot",
    "description": "Tulpar Discord Bot.",
    "version": "v1.4.7",
    "keywords": [
        "tulpar",
        "discord",
        "bot"
    ],
    "homepage": "https://github.com/isaeken/tulpar",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Isa Eken",
            "email": "hello@isaeken.com.tr",
            "homepage": "https://www.isaeken.com.tr",
            "role": "Founder"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-gd": "*",
        "bensampo/laravel-enum": "^3.4",
        "consoletvs/profanity": "^3.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "geerlingguy/ping": "^1.2",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/database": "^8.40",
        "illuminate/http": "^8.0",
        "intervention/image": "^2.6",
        "isaeken/strargs": "^2.4.0",
        "laravel/framework": "^8.52",
        "norkunas/youtube-dl-php": "^2.0",
        "socialiteproviders/discord": "^4.1",
        "socialiteproviders/zoho": "^4.1",
        "spatie/color": "^1.2",
        "team-reflex/discord-php": "dev-master",
        "xpaw/php-source-query-class": "^2.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "tulpar"
    ]
}
